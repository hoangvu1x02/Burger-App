
<div class="container">
    <div class="img-container">
        <img src="https://miro.medium.com/max/1000/1*OL4q5Jrem4nnD3BuDHg7wg.png" alt="">
    </div>
    <h1 class="title">Eat-Da-Burger</h1>
    <div class="lists-container">
        <ul>
            <h3>Not Devoured</h3>
            {{#each burgers}}
            {{#with dataValues}}
            {{#unless isDevoured}}
            <li>Name: {{name}}
                <button class="devour" data-id="{{ id }}">Devour It</button>
            </li>
            {{/unless}}
            {{/with}}
            {{/each}}
        </ul>
        <ul>
            <h3>Devoured</h3>
            {{#each burgers}}
            {{#if dataValues.isDevoured}}
            <li>Name: {{dataValues.name}}</li>
            {{/if}}
            {{/each}}
        </ul>
    </div>
    <div class="input">
        <input type="text" id="newBurger"> <button id="add">Add Burger</button>
    </div>
</div>
